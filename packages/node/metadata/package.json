    "generate:contributor": "tsx src/contributor.ts",
    "generate:components": "tsx src/components.ts",
    "dev": "DEV=1 pnpm run build"
  },
  "devDependencies": {
    "@element-plus/build": "^0.0.1",
    "@element-plus/build-utils": "^0.0.1",
    "@element-plus/node-constants": "workspace:*",
    "@types/lodash-es": "^4.17.6",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "fast-glob": "^3.2.11",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "octokit": "^2.0.3",
    "tsx": "^3.6.0"
  }
}
